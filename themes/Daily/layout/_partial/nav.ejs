<div id="local-search-result"></div>
<div class="navbox">
  <!-- Navigation -->
  <nav class="navbar">
    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse">
      <ul class="navbar-nav">
        <% var site_navs = config.nav || theme.nav  %>
          <% for (var i in site_navs){ %>
            <li>
              <a href="<%- site_navs[i].url ? url_for(site_navs[i].url) : url_for(site_navs[i]) %>">
                <% if (site_navs[i].icon) { %>
                  <i class="fa fa-<%= site_navs[i].icon %> fa-stack-2x"></i>
                  <% } else { %>
                    <%= i %>
                      <% } %>
              </a>
            </li>
            <% } %>
      </ul>
    </div>
    <!-- /.navbar-collapse -->
  </nav>
<div class="menu">
<i class="iconfont icon-menu"></i>
</div>
<div class="search">
  <div id="site_search">
    <i class="iconfont icon-search"></i>
    <input type="text" id="local-search-input" name="q" results="0" class="form-control" />
  </div>
</div>
</div>

<header class="site-header">
    <div class="logo">
      <a href="<%= url_for() %>" rel="home">
        <% if (config.logo || theme.logo) { %>
        <img src="<%- config.logo || theme.logo %>" alt="<%- config.title %>" height="60">
        <% } %>
      </a>
      <h1>
        <a href="<%= url_for() %>" rel="home">
            <%= config.title %>
        </a>
      </h1>
      <h5><%= config.subtitle %></h5>
      <div class="contact">
      <a href="https://github.com/leiadot">
        <i class="iconfont icon-github"></i>
        <span>Github</span>
      </a>
      <a href="mailto:luv.hsu@gmail.com">
        <i class="iconfont icon-mail"></i>
        <span>Mail</span>
      </a>
      <a href="https://www.facebook.com/CodingWifeHy2/">
        <i class="iconfont icon-facebook"></i>
        <span>Facebook</span>
      </a>
      <a href="/tagcloud">
        <i class="iconfont icon-tags"></i>
        <span>tagcloud</span>
      </a>
      </div>
  </div>
</header>

