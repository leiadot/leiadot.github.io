<div class="navbox">
  <!-- Navigation -->
  <nav class="navbar">
    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse">
      <ul class="navbar-nav">
        <% var site_navs = config.nav || theme.nav  %>
          <% for (var i in site_navs){ %>
            <li>
              <a href="<%- site_navs[i].url ? url_for(site_navs[i].url) : url_for(site_navs[i]) %>">
                <% if (site_navs[i].icon) { %>
                  <i class="fa fa-<%= site_navs[i].icon %> fa-stack-2x"></i>
                  <% } else { %>
                    <%= i %>
                      <% } %>
              </a>
            </li>
            <% } %>
              <li>
                <div id="site_search">
                  <input type="text" id="local-search-input" name="q" results="0" placeholder="search" class="form-control" />
                  <div id="local-search-result"></div>
                </div>
              </li>
              <% if (config.social) { %>

                <% _.forIn(config.social, function(val, key){ %>
                  <li>

                    <a href="<%- val %>" class="icon icon-<%- key %>" target="_blank">
                      <%- key %>
                    </a>
                  </li>
                  <% }) %>
                    <% } else { %>
                      <% _.forIn(theme.social, function(val, key){ %>
                        <li>
                          <a href="<%- val %>" class="icon icon-<%- key %>" target="_blank">
                            <%- key %>
                          </a>
                          <%- key %>
                        </li>
                        <% }) %>
                          <% } %>
      </ul>

    </div>
    <!-- /.navbar-collapse -->
  </nav>


</div>

<header class="site-header">
    <div class="logo">
      <a href="<%= url_for() %>" rel="home">
        <% if (config.logo || theme.logo) { %>
        <img src="<%- config.logo || theme.logo %>" alt="<%- config.title %>" height="60">
        <% } %>
      </a>
      <h1><%= config.title %></h1>
      <h5><%= config.subtitle %></h5>
  </div>
</header>

